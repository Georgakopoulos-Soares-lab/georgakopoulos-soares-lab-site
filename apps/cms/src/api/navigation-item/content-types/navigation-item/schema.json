{
  "kind": "collectionType",
  "collectionName": "navigation_items",
  "info": {
    "singularName": "navigation-item",
    "pluralName": "navigation-items",
    "displayName": "Navigation Item"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "label": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "href": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true,
      "regex": "^\\/[a-zA-Z0-9\\-\\/]*$"
    },
    "order": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "target": {
      "type": "enumeration",
      "enum": [
        "_self",
        "_blank",
        "_parent",
        "_top"
      ],
      "default": "_self"
    }
  }
}
